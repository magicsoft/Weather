source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!

abstract_target 'WeatherTargets' do

    # Common pods
    pod 'SwiftyJSON', '~> 2.3'

    target 'Weather' do
        pod 'Alamofire', '~> 3.4'
        pod 'AlamofireImage', '~> 2.4'
        pod 'XCGLogger', '~> 3.3'
    end

    target 'WeatherTests'

end
